  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <pages>
      <controls>
        <add tagPrefix="customControls" tagName="Footer" src="~/Controls/footer.ascx"></add>
        <add tagPrefix="customControls" tagName="Header" src="~/Controls/header.ascx"></add>
        <add tagPrefix="customControls" tagName="headerStrip" src="~/Controls/headerStrip.ascx"></add>
      </controls>
    </pages>
  </system.web>

