<configuration>
  <connectionStrings>
    <add name="coachingPageConnection" connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\CoachIn.mdf;Initial Catalog=CoachIn;Integrated Security=True"
        providerName="System.Data.SqlClient" />
